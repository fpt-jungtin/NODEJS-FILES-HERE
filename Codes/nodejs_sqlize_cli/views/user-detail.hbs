<div>
    <ul>
        <li>ID : {{user.id}}</li>
        <li>Email : {{user.email}}</li>
    </ul>
</div>
<table>
    <tr>
        <td>ID</td>
        <td>Title</td>
        <td>Content</td>
    </tr>
    {{#each user.Posts as |post|}}
    <tr>
        <td>{{post.id}}</td>
        <td>{{post.title}}</td>
        <td>{{post.content}}</td>
    </tr>
    {{/each}}
</table>